          <TableColumn fx:id="tableColumnPrenume" prefWidth="110.6" text="First Name" />
        </columns>
      </TableView>
      <Label layoutX="77.0" layoutY="71.0" prefHeight="27.0" prefWidth="99.0" text="Your friends">
